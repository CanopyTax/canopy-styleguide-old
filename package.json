{
  "name": "canopy-styleguide",
  "version": "5.1.12",
  "description": "Style-guide for Canopy",
  "main": "build/styleguide.css",
  "scripts": {
    "setup": "bin/install-git-hooks post-merge",
    "clean": "rm -rf build/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && npm run build-icons && node node_modules/webpack/bin/webpack.js --config webpack.config.build-css.js && npm run build-index",
    "build-index": "cp index.html build/",
    "build-icons": "fontcustom compile -F && node bin/build-fonts.js",
    "deploy": "cp build/* ../workflow-ui/node_modules/canopy-styleguide/build/",
    "website": "webpack-dev-server",
    "prepublish": "npm run build",
    "start": "http-server sofe --cors -c-1 --ssl --cert public.pem --key key.pem -p",
    "watch": "echo 'Please ensure fontcustom is installed\n' && npm run build-icons && ./bin/build -w",
    "sofe": "rm -rf build && npm run build-icons && ./bin/build -p"
  },
  "git-tasker": [
    {
      "matcher": "package.json",
      "command": "npm install"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/canopytax/styleguide.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canopytax/styleguide/issues"
  },
  "homepage": "https://github.com/canopytax/styleguide",
  "devDependencies": {
    "autoprefixer-loader": "1.2.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "0.3.8",
    "file-loader": "0.8.4",
    "git-tasker": "^1.0.1",
    "highlight.js": "^8.4.0",
    "html-loader": "^0.2.3",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.2",
    "less": "2.4.0",
    "less-loader": "2.1.0",
    "lodash": "^3.5.0",
    "qs": "^5.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-highlight": "^0.5.0",
    "react-router": "1.0.3",
    "replace": "^0.3.0",
    "rework": "^1.0.1",
    "sofe-css-builder": "^1.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "0.5.6",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "1.0.0-alpha.3",
    "history": "1.17.0"
  }
}
