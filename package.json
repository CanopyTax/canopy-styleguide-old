{
  "name": "canopy-styleguide",
  "version": "4.1.33",
  "description": "Style-guide for Canopy",
  "main": "build/styleguide.css",
  "scripts": {
    "setup": "bin/install-git-hooks post-merge",
    "clean": "rm build/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.build.js && npm run build-index",
    "build-index": "cp index.html build/",
    "deploy": "cp build/* ../workflow-ui/node_modules/canopy-styleguide/build/",
    "patch-workflowui": "echo 'Please ensure that your git working directory is clean' && rm -rf node_modules && npm install && npm version patch -m '%s -- npm run patch-workflowui has incremented the patch version of canopy-styleguide to %s' && npm publish && cd ../workflow-ui && npm prune && npm install && npm install canopy-styleguide@latest --save --save-exact && npm shrinkwrap && echo 'Please make sure to commit and push any changed built files to source control'",
    "start": "webpack-dev-server",
    "test-workflow-ui": "npm run build && cd ../workflow-ui && npm i ../canopy-styleguide && cd ../canopy-styleguide"
  },
  "git-tasker": [
    {
      "matcher": "package.json",
      "command": "npm install"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/canopytax/styleguide.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canopytax/styleguide/issues"
  },
  "homepage": "https://github.com/canopytax/styleguide",
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "canopy-iconography": "1.7.6",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.4",
    "git-tasker": "^1.0.1",
    "highlight.js": "^8.4.0",
    "html-loader": "^0.2.3",
    "json-loader": "^0.5.2",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "lodash": "^3.5.0",
    "qs": "^5.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-highlight": "^0.5.0",
    "react-router": "1.0.3",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.7.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {
    "canopy-iconography": "1.7.6",
    "history": "1.17.0"
  },
  "publishConfig": {
    "registry": "http://npm.canopytax.com"
  }
}
