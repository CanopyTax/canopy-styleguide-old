{
  "name": "canopy-styleguide",
  "description": "Styleguide for Canopy",
  "scripts": {
    "setup": "bin/install-git-hooks post-merge",
    "clean": "rm -rf sofe build website fonts",
    "test": "karma start",
    "test-ci": "karma start karma-saucelabs.conf.js",
    "build-icons": "fontcustom compile -F && node bin/build-fonts.js",
    "local-website": "npm run build-icons && webpack-dev-server --config webpack.config.website.js",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --https --cert ~/.canopy-ssl/public.pem --key ~/.canopy-ssl/key.pem --port",
    "sofe": "./bin/build -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canopytax/styleguide.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canopytax/styleguide/issues"
  },
  "homepage": "https://github.com/canopytax/styleguide",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chroma-js": "^1.3.4",
    "css-loader": "^0.28.7",
    "custom-event-polyfill": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "git-tasker": "^1.0.1",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.1",
    "js-beautify":"^1.7.4",
    "jasmine": "^2.8.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "lodash": "^4.17.4",
    "postcss-loader": "^2.0.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-element-to-jsx-string": "^13.0.0",
    "react-highlight": "^0.10.0",
    "react-router-dom": "4.2.2",
    "replace": "^0.3.0",
    "rework": "^1.0.1",
    "sofe-css-builder": "^1.0.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@joeldenning/custom-elements": "1.0.0-alpha.6",
    "color": "^2.0.1",
    "history": "^4.7.2",
    "moment": "^2.19.1",
    "preact": "^8.2.6"
  }
}
