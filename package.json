{
  "name": "canopy-styleguide",
  "description": "Styleguide for Canopy",
  "scripts": {
    "setup": "bin/install-git-hooks post-merge",
    "clean": "rm -rf sofe build website fonts",
    "test": "karma start",
    "test-ci": "karma start karma-saucelabs.conf.js",
    "build-icons": "fontcustom compile -F && node bin/build-fonts.js",
    "local-website": "npm run build-icons && webpack-dev-server --config webpack.config.website.js",
    "start": "http-server sofe --cors -c-1 --ssl --cert ~/.canopy-ssl/public.pem --key ~/.canopy-ssl/key.pem -p",
    "watch": "echo 'Please ensure fontcustom is installed\n' && npm run build-icons && ./bin/build -w",
    "sofe": "./bin/build -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canopytax/styleguide.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canopytax/styleguide/issues"
  },
  "homepage": "https://github.com/canopytax/styleguide",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "custom-event-polyfill": "^0.3.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "git-tasker": "^1.0.1",
    "highlight.js": "^9.10.0",
    "html-loader": "^0.5.1",
    "jasmine": "^2.5.3",
    "json-loader": "^0.5.7",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lodash": "^4.17.4",
    "postcss-loader": "^2.0.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-highlight": "^0.9.0",
    "react-router": "1.0.3",
    "replace": "^0.3.0",
    "rework": "^1.0.1",
    "sofe-css-builder": "^1.0.2",
    "style-loader": "^0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "@joeldenning/custom-elements": "1.0.0-alpha.6",
    "color": "^1.0.3",
    "history": "1.17.0",
    "moment": "^2.18.1",
    "preact": "7.2.1"
  }
}
